{
  "_note": "This file is only for IDE. For build use tsconfig.[dev/build].json (dev mode is less strict)",
  "compilerOptions": {
    "declaration": true,
    "jsx": "react",
    "lib": [
      "es7",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "dist",
    "sourceMap": true,
    "types": ["jest", "node"],
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "node_modules"
  ]
}
